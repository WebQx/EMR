{
  "name": "WebQx Global - 24/7 Healthcare",
  "short_name": "WebQx Global",
  "description": "Global 24/7 telehealth platform connecting patients to healthcare providers worldwide",
  "start_url": "/telehealth-24-7-global.html",
  "display": "standalone",
  "background_color": "#2196F3",
  "theme_color": "#2196F3",
  "orientation": "portrait-primary",
  "scope": "/",
  "lang": "en-US",
  "dir": "ltr",
  
  "icons": [
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "48x48",
      "type": "image/svg+xml"
    },
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "96x96",
      "type": "image/svg+xml"
    },
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "192x192",
      "type": "image/svg+xml",
      "purpose": "any maskable"
    },
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "512x512",
      "type": "image/svg+xml"
    }
  ],

  "categories": ["medical", "health", "productivity", "lifestyle"],
  
  "screenshots": [
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "390x680",
      "type": "image/svg+xml",
      "form_factor": "narrow",
      "label": "Mobile view of WebQx Global telehealth platform"
    }
  ],

  "shortcuts": [
    {
      "name": "Emergency Consultation",
      "short_name": "Emergency",
      "description": "Start emergency medical consultation",
      "url": "/telehealth-24-7-global.html#emergency",
      "icons": [
        {
          "src": "data:image/svg+xml;base64,",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Schedule Appointment",
      "short_name": "Schedule",
      "description": "Book a medical appointment",
      "url": "/telehealth-24-7-global.html#schedule",
      "icons": [
        {
          "src": "data:image/svg+xml;base64,",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Crisis Support",
      "short_name": "Crisis",
      "description": "24/7 mental health crisis support",
      "url": "/telehealth-24-7-global.html#crisis",
      "icons": [
        {
          "src": "data:image/svg+xml;base64,",
          "sizes": "96x96"
        }
      ]
    }
  ],

  "prefer_related_applications": false,
  
  "related_applications": [
    {
      "platform": "webapp",
      "url": "https://webqx.com/"
    }
  ],

  "protocol_handlers": [
    {
      "protocol": "web+telehealth",
      "url": "/telehealth-24-7-global.html?consultation=%s"
    }
  ],

  "file_handlers": [
    {
      "action": "/import-medical-data",
      "accept": {
        "application/fhir+json": [".fhir", ".json"],
        "text/csv": [".csv"],
        "application/pdf": [".pdf"]
      }
    }
  ],

  "share_target": {
    "action": "/share-medical-data",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text", 
      "url": "url",
      "files": [
        {
          "name": "medical_files",
          "accept": ["image/*", "application/pdf", "application/fhir+json"]
        }
      ]
    }
  },

  "capture_links": "new-client",
  
  "launch_handler": {
    "client_mode": "focus-existing"
  },

  "edge_side_panel": {
    "preferred_width": 400
  },

  "features": [
    "cross-origin-isolated"
  ],

  "permissions": [
    "camera",
    "microphone", 
    "notifications",
    "geolocation",
    "background-sync",
    "background-fetch"
  ]
}
