{
  "name": "webqx-global-analytics",
  "version": "1.0.0",
  "description": "Real-time analytics dashboard for WebQX global healthcare platform serving millions of users",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "docker:build": "docker build -t webqx-analytics .",
    "docker:run": "docker run -p 3002:3002 -p 8080:8080 webqx-analytics",
    "k8s:deploy": "kubectl apply -f k8s/",
    "production": "NODE_ENV=production node server.js"
  },
  "keywords": [
    "healthcare",
    "analytics",
    "real-time",
    "dashboard",
    "webqx",
    "global",
    "metrics",
    "websocket"
  ],
  "author": "WebQX Analytics Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "mysql2": "^3.6.5",
    "ws": "^8.14.2",
    "geoip-lite": "^1.4.10",
    "uuid": "^9.0.1",
    "helmet": "^7.1.0",
    "rate-limiter-flexible": "^2.4.2",
    "winston": "^3.11.0",
    "node-cron": "^3.0.3",
    "compression": "^1.7.4",
    "express-rate-limit": "^6.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "@types/node": "^20.9.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/webqx/global-analytics.git"
  },
  "bugs": {
    "url": "https://github.com/webqx/global-analytics/issues"
  },
  "homepage": "https://analytics.webqx.healthcare"
}
