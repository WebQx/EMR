{
  "name": "webqx-healthcare-platform",
  "version": "1.0.0",
  "description": "WebQXâ„¢: Modular Healthcare Platform - A multilingual, specialty-aware, and privacy-first blueprint for global clinical care",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "precommit": "npm run lint && npm run format:check && npm run type-check",
    "security:audit": "npm audit --audit-level=moderate",
    "security:check": "npm run security:audit && npm run lint",
    "prepare": "husky install",
    "validate:branch-protection": "node scripts/validate-branch-protection.js"
  },
  "keywords": [
    "healthcare",
    "medical",
    "webqx",
    "patient-portal",
    "healthcare-platform"
  ],
  "author": "WebQX Health",
  "license": "Apache-2.0",
  "dependencies": {
    "@types/fhir": "^0.0.40",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "fhir": "^4.11.1",
    "helmet": "^7.1.0",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "react-i18next": "^15.6.1",
    "uuid": "^9.0.1"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/cors": "^2.8.17",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-security": "^3.0.1",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "node scripts/validate-commit-msg.js"
    }
  }
}
