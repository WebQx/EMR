{
  "name": "webqx-healthcare-platform",
  "version": "1.0.0",
  "description": "WebQXâ„¢: Modular Healthcare Platform - A multilingual, specialty-aware, and privacy-first blueprint for global clinical care",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "deploy:lambda": "node scripts/deploy-lambda.js",
    "telehealth:start:full": "cd modules/telehealth/deployment/full-suite && docker-compose up -d",
    "telehealth:start:video": "cd modules/telehealth/deployment/standalone/video-consultation && docker-compose up -d",
    "telehealth:start:messaging": "cd modules/telehealth/deployment/standalone/messaging && docker-compose up -d",
    "telehealth:start:ehr": "cd modules/telehealth/deployment/standalone/ehr-integration && docker-compose up -d",
    "telehealth:start:fhir": "cd modules/telehealth/deployment/standalone/fhir-sync && docker-compose up -d",
    "telehealth:stop": "cd modules/telehealth/deployment && find . -name docker-compose.yml -execdir docker-compose down \\;",
    "telehealth:test": "jest modules/telehealth/tests/",
    "telehealth:health": "curl -f http://localhost:3000/health/telehealth",
    "chatehr:mock": "node scripts/start-mock-chatehr.js",
    "chatehr:test": "jest __tests__/chatEHR.test.js",
    "chatehr:health": "curl -f http://localhost:3000/api/chatehr/health",
    "build:css": "tailwindcss -i ./src/styles.css -o ./assets/webqx-styles.css --watch",
    "build:css:prod": "tailwindcss -i ./src/styles.css -o ./assets/webqx-styles.css --minify",
    "build:pages": "node scripts/build-pages.js",
    "deploy:pages": "npm run build:css:prod && npm run build:pages"
  },
  "keywords": [
    "healthcare",
    "medical",
    "webqx",
    "patient-portal",
    "healthcare-platform"
  ],
  "author": "WebQX Health",
  "license": "Apache-2.0",
  "dependencies": {
    "@azure/identity": "^4.10.2",
    "@babel/preset-env": "^7.28.0",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/fhir": "^0.0.40",
    "@types/keycloak-connect": "^4.5.4",
    "@types/keycloak-js": "^2.5.4",
    "@xmldom/xmldom": "^0.9.8",
    "argon2": "^0.43.1",
    "axios": "^1.11.0",
    "babel-jest": "^30.0.5",
    "bcrypt": "^6.0.0",
    "connect-redis": "^9.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.2",
    "express-validator": "^7.2.1",
    "fhir": "^4.11.1",
    "helmet": "^7.1.0",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "joi": "^17.11.0",
    "jose": "^6.0.12",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "keycloak-connect": "^26.1.1",
    "keycloak-js": "^26.2.0",
    "react-i18next": "^15.6.1",
    "redis": "^5.6.1",
    "ts-node": "^10.9.2",
    "uuid": "^9.0.1",
    "ws": "^8.18.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/cors": "^2.8.17",
    "@types/express-validator": "^2.20.33",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/uuid": "^9.0.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3"
  }
}
